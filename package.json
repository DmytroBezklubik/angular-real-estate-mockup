{
  "name": "kiwi-dom",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "node serve-script",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run kiwi-dom:server:production",
    "build:ssr-dev": "npm run build:client-and-server-bundles-dev && npm run compile:server",
    "build:client-and-server-bundles-dev": "ng build && ng run kiwi-dom:server",
    "compile_@agm_core": "babel node_modules/@agm/core -d node_modules/@agm/core --presets @babel/preset-env",
    "postinstall": "npm run compile_@agm_core"
  },
  "private": true,
